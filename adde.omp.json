{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#E5C07B",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": "[{{ .CurrentDate | date .Format }}]",
          "type": "time"
        },
        {
          "type": "shell",
          "style": "plain",
          "foreground": "#E06C75",
          "properties": {
            "mapped_shell_names": {
              "pwsh": "Pwsh",
              "powershell": "Shell",
              "cmd": "Cmd",
              "bash": "Bash"
            }
          },
          "template": " \ue795 {{ .Name }} {{ replaceP \".[0-9].*\" .Version \"\" }} "
        },
        {
          "foreground": "#1bd1f2",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#5a337d",
          "style": "diamond",
          "template": "[\ue266</> <#be9ddf>RAM: {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB</>] ",
          "type": "sysinfo"
        },
        {
          "foreground": "#a30217",
          "foreground_templates": [
            "{{if lt .Percentage 20}}#e51f1f{{end}}",
            "{{if lt .Percentage 40}}#f2a134{{end}}",
            "{{if lt .Percentage 60}}#f7e379{{end}}",
            "{{if lt .Percentage 80}}#bbdb44{{end}}",
            "{{if lt .Percentage 100}#44ce1b{{end}}"
          ],
          "properties": {
            "discharging_icon": "\udb84\udea2",
            "charging_icon": "\udb80\udc88",
            "charged_icon": "\udb84\udea3"
          },
          "style": "plain",
          "template": "<#333333>[</>{{ if not .Error }}{{ end }}{{if and (gt .Percentage 0) (lt .Percentage 10)}}{{if eq \"Charging\" .State.String}}\udb82\udc9c{{else}}\udb80\udc8e{{end}}{{end}}{{if and (gt .Percentage 10) (lt .Percentage 20)}}{{if eq \"Charging\" .State.String}}\udb80\udc86{{else}}\udb80\udc7a{{end}}{{end}}{{if and (gt .Percentage 20) (lt .Percentage 30)}}{{if eq \"Charging\" .State.String}}\udb80\udc86{{else}}\udb80\udc7b{{end}}{{end}}{{if and (gt .Percentage 30) (lt .Percentage 40)}}{{if eq \"Charging\" .State.String}}\udb80\udc87{{else}}\udb80\udc7c{{end}}{{end}}{{if and (gt .Percentage 40) (lt .Percentage 45)}}{{if eq \"Charging\" .State.String}}\udb80\udc88{{else}}\udb80\udc7d{{end}}{{end}}{{if and (gt .Percentage 45) (lt .Percentage 55)}}{{if eq \"Charging\" .State.String}}\udb82\udc9d{{else}}\udb80\udc7e{{end}}{{end}}{{if and (gt .Percentage 55) (lt .Percentage 65)}}{{if eq \"Charging\" .State.String}}\udb80\udc89{{else}}\udb80\udc7f{{end}}{{end}}{{if and (gt .Percentage 65) (lt .Percentage 75)}}{{if eq \"Charging\" .State.String}}\udb82\udc9e{{else}}\udb80\udc80{{end}}{{end}}{{if and (gt .Percentage 75) (lt .Percentage 85)}}{{if eq \"Charging\" .State.String}}\udb80\udc8a{{else}}\udb80\udc81{{end}}{{end}}{{if and (gt .Percentage 84) (lt .Percentage 95)}}{{if eq \"Charging\" .State.String}}\udb80\udc8b{{else}}\udb80\udc82{{end}}{{end}}{{if and (gt .Percentage 95) (lt .Percentage 101)}}{{if eq \"Charging\" .State.String}}\udb84\udea6{{else}}\udb80\udc79{{end}}{{end}} {{ .Percentage }}{{ .Error }}\uf295 <#333333>]</> ",
          "type": "battery"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "terraform",
          "style": "diamond",
          "powerline_symbol": "",
          "foreground": "#0a6778",
          "template": "{{ if eq .Env.OMP_ADDE_TF \"True\" }}[ <#1bd1f2>\udb84\udc62 {{ .WorkspaceName }}{{ if .Version }} {{ .Version }}{{ end }}</> ] {{end}}"
        },
        {
          "foreground": "#0a6778",
          "style": "diamond",
          "template": "{{ if eq .Env.OMP_ADDE_AZURE \"True\" }}[<#1bd1f2>\uebd8  {{ .Name }} Teneant: {{ .TenantDisplayName }} ({{ .TenantID }})</><#0a6778>]</> {{ end }}",
          "type": "az"
        },
        {
          "foreground": "#1bd1f2",
          "style": "diamond",
          "template": "{{ if eq .Env.OMP_ADDE_AZURE \"True\" }}[<#ffffff>\uebd8</> Azure Developer CLI Environment: <#0a6778>{{ .DefaultEnvironment  }}</> :: <#0a6778>{{ .Version }}</>] {{ end }}",
          "type": "azd"
        },
        {
          "foreground": "#0a6778",
          "style": "diamond",
          "template": "{{ if eq .Env.OMP_ADDE_AWS \"True\" }}[<#1bd1f2>\uf270 {{ .Profile }}{{if .Region}}@{{ .Region }}</>{{ end }}]{{ end }}",
          "type": "aws"
        },
        {
          "foreground": "#1bd1f2",
          "style": "diamond",
          "template": "{{ if eq .Env.OMP_ADDE_GCP \"True\" }}[{{ if .Error }}{{ .Error }}{{ else }}<#ffffff>\uf1a0</> <#0a6778>{{ .Project }}</> :: <#0a6778>{{.Account}}</>{{ end }}] {{ end }}",
          "type": "gcp"
        },
        {
          "foreground": "#0a6778",
          "style": "diamond",
          "template": "{{ if eq .Env.OMP_ADDE_PROJECT \"True\" }}[{{ if .Error }}{{ .Error }}{{ else }}{{ if .Name }}<#1bd1f2>\uf487 {{ .Name }}{{ end }}{{ if .Target }} {{ .Target }}{{ end }}</>{{ end }}] {{ end }}",
          "type": "project"
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "#b8ff75",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ],
          "template": " x{{ reason .Code }}"
        },
        {
          "foreground": "#b8ff75",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ],
          "properties": {
            "style": "roundrock",
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#61AFEF",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "\ue5ff {{ .Path }}",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#1bd1f2",
          "style": "plain",
          "template": "{{ if eq .Env.OMP_ADDE_KUBECTL \"True\" }}[<#0c404a>\udb84\udcfe</> <#0a6778>{{.Context}}</>{{if .Namespace}} :: <#0a6778>{{.Namespace}}</>{{end}}]{{end}}",
          "type": "kubectl"
        },
        {
          "type": "docker",
          "style": "plain",
          "display_mode": "files",
          "extensions": [
            "compose.yml",
            "compose.yaml",
            "docker-compose.yml",
            "docker-compose.yaml",
            "Dockerfile"
          ],
          "foreground": "#0c404a",
          "template": "{{ if eq .Env.OMP_ADDE_DOCKER \"True\" }}   {{ .Context }} {{end}}"
        },
        {
          "type": "connection",
          "style": "plain",
          "foreground": "#1bd1f2",
          "template": "{{ if eq .Type \"wifi\"}}\udb82\udd25 {{ else if eq .Type \"ethernet\"}}\ueba9{{ end }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "\uea86 ",
          "type": "root"
        },
        {
          "type": "os",
          "display_distro_name": true,
          "style": "plain",
          "template": "{{ if eq .OS \"windows\" }}<#1e89b0>{{end}}{{if eq .OS \"debian\"}}<#8c2f8f>{{end}}{{if eq .OS \"ubuntu\"}}<#9e3320>{{end}}{{if eq .Root true}}<#6e2c2a>{{end}}{{.Icon}} <#333333>\u276f"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3,
  "shell_integration": true,
  "async": true
}
